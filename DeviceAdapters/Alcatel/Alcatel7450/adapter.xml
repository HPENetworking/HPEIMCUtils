<adapter name="Alcatel7450">
	<description>Alcatel 7450, 7705, 7710, 7750, 7950 series switches,routers TiMOS version 3.x - 11.x</description>
	<version>1.0.0</version>
	<services>
		<service name="CLICommon">
			<item type="common">Alcatel7450_Common_CLI.xml</item>
		</service>

		<service name="CleanupParser">
			<item type="parser_definition">Alcatel7450_Cleanup_Parser.xml</item>
			<item type="perl_script">Alcatel7450_Cleanup_Parser_Script.pl</item>
		</service>

		<service name="ConfigBackup">
			<item type="builder_definition">Alcatel7450_Config_Backup_Builder.xml</item>
			<item type="tcl_script">Alcatel7450_Config_Backup_Builder_Script.xml</item>
		</service>

		<service name="ConfigDeploy">
			<item type="builder_definition">Alcatel7450_Config_Deploy_Builder.xml</item>
			<item type="tcl_script">Alcatel7450_Config_Deploy_Builder_Script.xml</item>
		</service>
	</services>
</adapter>

