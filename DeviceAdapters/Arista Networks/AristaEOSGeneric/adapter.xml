
<adapter name="AristaEOSGeneric">
	<description>Arista EOS Version 4.x </description>
	<version>1.0.0</version>
	<services>
		<service name="CLICommon">
			<item type="common">Arista_Common_CLI.xml</item>
		</service>

		<service name="CleanupParser">
			<item type="parser_definition">Arista_Cleanup_Parser.xml</item>
			<item type="perl_script">Arista_Cleanup_Parser_Script.pl</item>
		</service>

		<service name="ConfigBackup">
			<item type="builder_definition">Arista_Config_Backup_Builder.xml</item>
			<item type="tcl_script">Arista_Config_Backup_Builder_Script.xml</item>
		</service>

		<service name="ConfigDeploy">
			<item type="builder_definition">Arista_Config_Deploy_Builder.xml</item>
			<item type="tcl_script">Arista_Config_Deploy_Builder_Script.xml</item>
		</service>

		<service name="ImageDeploy">
			<item type="builder_definition">Arista_Image_Deploy_Builder.xml</item>
			<item type="tcl_script">Arista_Image_Deploy_Builder_Script.xml</item>
		</service>

	</services>
</adapter>

